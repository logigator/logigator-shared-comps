{
	"name": "@logigator/logigator-shared-comps",
	"version": "0.0.0",
	"license": "MIT",
	"description": "Component library for Logigator",
	"scripts": {
		"ng": "ng",
		"start": "ng build --watch -c ivy",
		"start:view-engine": "ng build --watch",
		"build": "ng build",
		"publish-package-actions": "npm run build && npm run publish-to-npm",
		"publish-package": "npm run bump-version && npm run build && npm run publish-to-npm",
		"bump-version": "cd projects/logigator-shared-comps && npm version patch --no-git-tag-version",
		"publish-to-npm": "cd dist/logigator-shared-comps && npm publish --access public",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/logigator/logigator-shared-comps.git"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "~10.1.0",
		"@angular/common": "~10.1.0",
		"@angular/compiler": "~10.1.0",
		"@angular/core": "~11.0.5",
		"@angular/forms": "~10.1.0",
		"@angular/platform-browser": "~10.1.0",
		"@angular/platform-browser-dynamic": "~10.1.0",
		"@angular/router": "~10.1.0",
		"@ngx-translate/core": "^13.0.0",
		"rxjs": "~6.5.4",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1001.0",
		"@angular-devkit/build-ng-packagr": "~0.1001.0",
		"@angular/cli": "~10.1.0",
		"@angular/compiler-cli": "~10.1.0",
		"@angular/language-service": "~10.1.0",
		"@types/node": "^12.11.1",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"codelyzer": "^5.1.2",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"ng-packagr": "^10.1.0",
		"protractor": "~7.0.0",
		"ts-node": "~7.0.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.2"
	}
}
